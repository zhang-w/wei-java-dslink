{
  "name": "wei-dslink-java",
  "version": "0.0.1",
  "description": "An Java DSLink by wei.",
  "license": "Apache",
  "author": {
    "name": "Wei Zhang",
    "email": "0496170019@tongji.edu.cn"
  },
  "main": "bin/wei-dslink-java",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/wei-dslink-java"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/wei-dslink-java/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "simulator"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.wei.Main"
    }
  }
}
